{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "*": [
            "*",
            "src/*"
            ]
        },
        "module": "commonjs",
        "target": "es5",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "removeComments": false,
        "jsx": "react",
        "forceConsistentCasingInFileNames": false,
        "strictNullChecks": false,
        "noImplicitThis": true,
        "allowSyntheticDefaultImports": true,
        "noUnusedLocals": true
    }
}